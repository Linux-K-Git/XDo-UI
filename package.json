{
  "name": "xdo-ui",
  "productName": "XDo-UI",
  "version": "2.2.9",
  "description": "A science fiction desktop running everywhere. Awesome.",
  "keywords": [
    "desktop",
    "sci-fi",
    "gui",
    "portable",
    "tty",
    "terminal"
  ],
  "main": "src/_boot.js",
  "scripts": {
    "start": "electron src --nointro",
    "install-windows": "npm install && cd src && npm install",
    "init-file-icons": "git submodule update --init",
    "update-file-icons": "git submodule foreach git pull origin master && node file-icons-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linux-K-Git/XDo-UI.git"
  },
  "author": "Linux-K https://github.com/Linux-K-Git/XDo-UI",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Linux-K-Git/XDo-UI/issues"
  },
  "homepage": "https://github.com/Linux-K-Git/XDo-UI#readme",
  "dependencies": {
    "clean-css": "5.2.1",
    "electron": "^12.1.0",
    "electron-rebuild": "^3.2.9",
    "ffi-napi": "^4.0.3",
    "mime-types": "^2.1.33",
    "node-abi": "2.30.1",
    "node-json-minify": "1.0.0",
    "ref-napi": "^3.0.3",
    "terser": "^5.9.0"
  },
  "optionalDependencies": {
    "cson-parser": "4.0.9"
  },
  "devDependencies": {}
}
